<aside class="aside-filters">
  <section>
    <input type="search" name="searchFilter" id="js-searchFilter" placeholder="Filter" autocomplete="off" class="d-none" />
  </section>

  <section>
    <form id="filter_form" action="/searchResults" method="get">
      <!-- genre -->
      <details class="main-genre"><summary>Genre<span class='material-icons'></span></summary>
    <div class="justify-start">
      <% Object.keys(genreList).forEach(function(main) { %>
        <details><summary><%= main %><span class='material-icons'></span></summary><div class="subgenres">
        <% genreList[main].forEach(genre => { %>
          <input type="checkbox" name="<%= genre %>" id="<%= genre %>">
          <label for="<%= genre %>"><%= genre %></label>
          <% }) %>
        </div>
        </details>
        <% }) %>
    </div>
      </details> 
      <!-- tempo -->
      <details><summary>Tempo<span class='material-icons'></span></summary>
      <div class="filter_tempo">
        <round-slider value="50" valueLabel="tempo" style="--round-slider-bar-color: #F20732;"></round-slider>
        <h1>Value: <span id="value">50</span></h1>
       <!-- <input type="range" name="tempo" id="tempo" min="30" max="240" step="5" list="tickmarks" value="30">
        <div><p>30</p><p>135</p><p>240</p></div>
       <label for="tempo">00 BPM</label> -->
      </div>
        <!-- <input type="number" name="" id="" pattern="\d{3}" placeholder=""> -->
        <!-- Dub: 60-90 bpm
          Hip-hop: 60-100 bpm
          House: 115-130 bpm
          Techno/trance: 120-140 bpm
          Dubstep: 135-145 bpm
          Drum and bass: 160-180 bpm -->
      </details>
      <!-- instrument -->
      <details><summary>instrument<span class='material-icons'></span></summary>
        <div class="subgenres">
          <input type="checkbox" name="guitar" id="guitar">
          <label for="guitar">Guitar</label>
          <input type="checkbox" name="flute" id="flute">
          <label for="flute">Flute</label>
          <input type="checkbox" name="drums" id="drums">
          <label for="drums">drums</label>
          <input type="checkbox" name="piano" id="piano">
          <label for="piano">piano</label>
        </div>
      </details>
      <!-- vocals -->
      <details><summary>Vocals<span class='material-icons'></span></summary>
      <div class="subgenres">
        <input type="checkbox" name="male" id="male">
        <label for="male">male</label>
        <input type="checkbox" name="female" id="female">
        <label for="female">female</label>
      </div>
      </details>
      <!-- key -->
      <details><summary>Key<span class='material-icons'></span></summary>
      <div>
        <input type="text" name="key" id="key" list="key_list" data-lpignore="true" placeholder="Search">
        <datalist id="key_list">
          <option>C</option>
          <option>D♭</option>
          <option>D</option>
          <option>E♭</option>
          <option>E</option>
          <option>G</option>
          <option>F#</option>
          <option>A♭</option>
          <option>A</option>
          <option>B♭</option>
          <option>B</option>
        </datalist>
      </div>
      </details>
      <!-- language -->
      <details><summary>Language<span class='material-icons'></span></summary>
      <div>
        <input type="text" name="lang" id="lang" list="lang_list" data-lpignore="true" placeholder="Search">
       <datalist id="lang_list">
         <option value="English">English</option>
         <option value="Dutch">Dutch</option>
         <option value="Spanish">Spanish</option>
         <option value="Hindi">Hindi</option>
       </datalist>
       </div>
      </details>
      <!-- mood -->
      <details><summary>Mood<span class='material-icons'></span></summary>
        <div class="subgenres">
          <input type="checkbox" name="sad" id="sad">
          <label for="sad">sad</label>
          <input type="checkbox" name="happy" id="happy">
          <label for="happy">happy</label>
          <input type="checkbox" name="agressive" id="agressive">
          <label for="agressive">agressive</label>
          <input type="checkbox" name="exciting" id="exciting">
          <label for="exciting">exciting</label>
        </div>
      </details>
      <!-- themes -->
      <details><summary>Themes<span class='material-icons'></span></summary>
        <div class="subgenres">
          <input type="checkbox" name="theme1" id="theme1">
          <label for="theme1">Theme 1</label>
          <input type="checkbox" name="theme2" id="theme2">
          <label for="theme2">theme 2</label>
          <input type="checkbox" name="theme3" id="theme3">
          <label for="theme3">theme 3</label>
          <input type="checkbox" name="theme4" id="theme4">
          <label for="theme4">theme 4</label>
        </div>
      </details>
      <!-- year -->
      <details><summary>Year<span class='material-icons'></span></summary>
    <div>
      <input type=number pattern="\d{4}" placeholder="2020" min="1700" max="2020" value="2020" name="year" id="year">
    </div>
      </details>
